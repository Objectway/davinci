doctype html
html

  block head

  body.Typo

    // Kitchen Sink menu toggler
    input.TogglerMenu#TogglerMenu(type="checkbox")

    aside.KitchenSinkSideBar
      .KitchenSinkSideBar__logo
        a(href="index.html") SuperGiGi
      block menu

    nav.KitchenSinkNavBar
      .KitchenSinkNavBar__menuBar
        .KitchenSinkNavBar__logo
          .KitchenSinkNavBar__title
            a(href="index.html") SuperGiGi
        .KitchenSinkNavBar__menuItems
          block menu

    nav.KitchenSinkTouchBar
      label.KitchenSinkTouchBar__label(for="TogglerMenu")
        svg.KitchenSinkTouchBar__icon#Livello_1(version='1.1', xmlns='http://www.w3.org/2000/svg', xmlns:xlink='http://www.w3.org/1999/xlink', x='0px', y='0px', viewbox='0 0 100 100', style='enable-background:new 0 0 100 100;', xml:space='preserve')
          rect#XMLID_1_(x='-0.1', y='70.3', width='100.1', height='15.5')
          rect#XMLID_4_(x='-0.1', y='42.7', width='100.1', height='15.5')
          rect#XMLID_3_(x='-0.1', y='14.8', width='100.1', height='16.3')
      .KitchenSinkTouchBar__logo
        .KitchenSinkTouchBar__title
          a(href="index.html") SuperGiGi

    .KitchenSinkOverlay
      label.KitchenSinkOverlay__label(for="TogglerMenu")

    .KitchenSinkBody
      block content

    .KitchenSinkFooter
      p.KitchenSinkFooter__paragraph
      .KitchenSinkFooter__versions.
        <img src="https://badge.fury.io/gh/objectway%2Fsuper-gigi.svg" alt="git v" />
        <img src="https://badge.fury.io/js/super-gigi.svg" alt="npm version" />


      script(
        type="text/javascript"
        src="development/scripts/vendor/prism.js"
      )
